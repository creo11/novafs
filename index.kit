<!DOCTYPE html>
<html lang="en">
<!-- @import "_head.kit"-->
<script>
	let copyrightYear = new Date().getFullYear();
</script>
<body>
<div class="main-content">
	<!-- @import "_nav.kit" -->
	<section id="hero" class="home">
		<div class="center-content">
			<h1>Trusted Cleaning & Maintenance for HOAs, Apartments & Commercial Spaces</h1>
			<div class="certifications">
				<img src="/img/home/certifications/ahca-premier-provider-award-2023.png">
				<img src="/img/home/certifications/ahca-covid.png">
				<img src="/img/home/certifications/ahca-phc.png">
				<img src="/img/home/certifications/ahca-registered-partner.png">
			</div>
		</div>
	</section>
	<section id="services">
		<div class="center-content">
			<div class="page-section-info">
				<h1 class="section-title">Services</h1>
				<h2>Maintain a Flawless First Impression</h2>
			</div>
			<div id="servicesContainer"></div>
		</div>
	</section>
	<!-- @import "_callToAction.kit"-->
</div>
<!-- @import "_footer.kit" -->
<template id="serviceItem.tpl.html">
	<div class="service-item">
		<div class="service-item-img"
		     style="background-image: url('${img}')">
		</div>
		<div class="service-details">
			<h3 class="service-title">${title}</h3>
			<div class="service-info">${info}</div>
			<button class="service-link"
			        onclick="window.location.href='${link}'">Learn More</button>
		</div>
	</div>
</template>
<script src="https://unpkg.com/@popperjs/core@2"></script>
<script src="https://unpkg.com/tippy.js@6"></script>
<script src="/js/nova-dist.js"></script>
<script src="/js/home-dist.js"></script>
</body>
</html>